<!-- Articles -->
<h1 class="mb-4 text-center">Latest Articles</h1>
<div class="container my-5 gap-4 d-flex flex-wrap articles-container">
  <% for(let i = 0; i < articles.length; i++) { %>
    <div class="card" style="width: 18rem">
        <img src="<%= articles[i].img %> " class="card-img-top" alt="No image available" />
        <div class="card-body">
        <h5 class="card-title"><%= articles[i].title %></h5>
        <p class="card-text my-0">By: <%= articles[i].author %></p>
        <p class="card-text my-0"><%= articles[i].date %></p>
        <a href="<%= articles[i].url %>" class="btn btn-primary">Read More</a>
        </div>
    </div>
  <% } %>
</div>

